{
    "name": "django-x509-dev",
    "dockerComposeFile": "../docker-compose.yaml",
    "service": "djangox509",
    "workspaceFolder": "/djangox509",
    "customizations": {
      "vscode":{
        "settings": {
          "terminal.integrated.defaultProfile.linux": "bash"
        },
        "extensions": [
          "ms-python.python",
          "ms-python.vscode-pylance"
        ]
      }
    },
    "postCreateCommand": "pip install -r requirements.txt || true && pip install -r /djangox509/getEAP_TLS/requirements.txt || true"
  }
  